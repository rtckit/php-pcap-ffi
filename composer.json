{
  "name": "rtckit/pcap-ffi",
  "description": "PHP Packet Capture via libpcap through FFI",
  "version": "0.6.2",
  "type": "library",
  "keywords": [
    "packet",
    "capture",
    "pcap",
    "libpcap",
    "tcpdump"
  ],
  "homepage": "https://github.com/rtckit/php-pcap-ffi",
  "license": "MIT",
  "authors": [
    {
      "name": "Ciprian Dosoftei"
    }
  ],
  "support": {
    "email": "hello@rtckit.io",
    "issues": "https://github.com/rtckit/php-pcap-ffi/issues"
  },
  "require": {
    "php": "^7.4",
    "ext-ffi": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3",
    "vimeo/psalm": "^3.15",
    "phpstan/phpstan": "^0.12"
  },
  "autoload": {
    "psr-4": {
      "RTCKit\\Pcap\\Stream\\FFI\\": "src/"
    },
    "files": [
      "src/register.php"
    ]
  },
  "autoload-dev": {
    "files": [
      "tests/helpers.php"
    ]
  }
}
